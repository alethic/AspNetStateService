<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" ApplicationTypeName="AspNetStateService.KeyShift" ApplicationTypeVersion="1.0.0" xmlns="http://schemas.microsoft.com/2011/01/fabric">
  <Parameters>
    <Parameter Name="Port" DefaultValue="42424" />
    <Parameter Name="Seq:ServerUrl" DefaultValue="" />
    <Parameter Name="Seq:ApiKey" DefaultValue="" />
    <Parameter Name="StateKeyShiftService_InstanceCount" DefaultValue="-1" />
  </Parameters>
  <ServiceManifestImport>
    <ServiceManifestRef ServiceManifestName="AspNetStateService.ServiceFabric.KeyShift.ServicesPkg" ServiceManifestVersion="1.0.0" />
    <ConfigOverrides>
      <ConfigOverride Name="Config">
        <Settings>
          <Section Name="Seq">
            <Parameter Name="ServerUrl" Value="[Seq:ServerUrl]" />
            <Parameter Name="ApiKey" Value="[Seq:ApiKey]" />
          </Section>
        </Settings>
      </ConfigOverride>
    </ConfigOverrides>
    <ResourceOverrides>
      <Endpoints>
        <Endpoint Name="HttpServiceEndpoint" Port="[Port]" />
      </Endpoints>
    </ResourceOverrides>
  </ServiceManifestImport>
  <DefaultServices>
    <Service Name="StateKeyShiftService" ServicePackageActivationMode="ExclusiveProcess">
      <StatelessService ServiceTypeName="StateKeyShiftService" InstanceCount="[StateKeyShiftService_InstanceCount]">
        <SingletonPartition />
      </StatelessService>
    </Service>
  </DefaultServices>
</ApplicationManifest>