{
    "AspNetStateService.AspNetCore": {
        "Store": "MongoDB"
    },
    "AspNetStateService.EntityFrameworkCore": {
        "Provider": "InMemory"
    },
    "AspNetStateService.Azure.Storage.Table": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "TableName": "state"
    },
    "AspNetStateService.Azure.Storage.Blob": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "ContainerName": "state"
    },
    "AspNetStateService.MongoDB": {
        "ConnectionString": "mongodb://localhost:27017"
    },
    "AspNetStateService.Redis": {
        "Configuration": "localhost"
    }
}
