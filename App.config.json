{
    "AspNetStateService.AspNetCore": {
        "Store": "KeyShift"
    },
    "AspNetStateService.EntityFrameworkCore": {
        "Provider": "InMemory"
    },
    "AspNetStateService.Azure.Storage.Table": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "TableName": "state"
    },
    "AspNetStateService.Azure.Storage.Blob": {
        "ConnectionString": "UseDevelopmentStorage=true",
        "ContainerName": "state"
    },
    "AspNetStateService.MongoDB": {
        "ConnectionString": "mongodb://localhost:27017"
    },
    "AspNetStateService.Redis": {
        "Configuration": "localhost"
    },
    "AspNetStateService.Amazon.S3": {
        "AwsAccessKeyId": "",
        "AwsSecretAccessKey": "",
        "RegionEndpointName": "us-east-1",
        "BucketName": "cogito-test"
    },
    "AspNetStateService.KeyShift": {
        "HostUri": "http://localhost:42428"
    }
}
